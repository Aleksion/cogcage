{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/checkout-success/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/events/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/founder-intent/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/ops/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/postback/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/replay-fallback/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/waitlist/?$",
			"dest": "_render"
		},
		{
			"src": "^/play/?$",
			"dest": "_render"
		},
		{
			"src": "^/success/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}